import random
import string

def generate_password(length):
    attributes = string.digits + string.ascii_letters + string.punctuation
    password = ''.join(random.choice(attributes) for i in range(length))
    return password

def main():
    length = int(input("Enter the required length of the password: "))
    password = generate_password(length)
    print("Required Password:", password)

if __name__ == "__main__":
    main()
